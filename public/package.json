  "scripts": {
    "server": "node ../appExpress.js",
    "lint": "eslint --ext .js,.vue --ignore-pattern marked.js --ignore-pattern webcomponents-hi-sd-ce.js webpack/appl webpack/tests",
    "build": "node webpack/build/build.js",
    "bpc": "rm -rf dist/brunch",
    "btc": "rm -rf dist_test/brunch",
    "bt": "npm run btc && brunch b --env test",
    "bp": "export LOGGY_STACKS=1; npm run bpc && brunch build --production",
    "bw": "npm run btc && brunch watch --server"
  },
