{
  "name": "vite",
  "version": "1.0.0",
  "scripts": {
    "watch": "npm-watch",
    "karma": "node runkarma",
    "dev": "vite --cors --base=./",
    "build": "vite build --base=./ && npm run copy",
    "buildi": "vite build --base=./ --mode=development && npm run copy",
    "serve": "vite preview",
    "acceptance": "cypress run -b chrome --headless",
    "acceptancet": "cypress open -b electron",
    "tdd": "concurrently --kill-others --success first --prefix '{index}' 'npm:dev' 'npm:acceptancet'",
    "test": "concurrently --kill-others --success first --prefix '{index}' 'start-test dev http-get://localhost:3000' 'npm:acceptance' ",
    "prod": "npm run test && npm run lint && npm run build && npm run integrationp",
    "lint": "eslint --fix --ext .js,.vue --ignore-pattern marked.js --ignore-pattern webcomponents-hi-sd-ce.js src/appl src/tests e2e",
    "touch": "touch touch.file",
    "touchwin": "copy touch.file+",
    "integration": "npm run startkoa && export INTEGRATION=true && npm run build && npm run karma && npm run stopkoa",
    "integrationp": "npm run startkoa && export INTEGRATION=true && npm run karma && npm run stopkoa",
    "startkoa": "cd ../.. && npm start &",
    "startkoaw": "cd ..\\.. && start /B \"\" npm start",
    "stopkoa": "cd ../.. && npm stop",
    "copy": "copyfiles README.md dist && copyfiles -u 1 src/images/**/* ./ && copyfiles -u 2  src/appl/views/**/* src/appl/templates/**/*  src/appl/dodex/**/* dist && copyfiles -u 2 src/appl/templates/* dist && copyfiles -u 1 src/images/**/* dist"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.2.0-6",
    "@fortawesome/fontawesome-free-solid": "^5.1.0-3",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "bootstrap": "^5.1.3",
    "browserslist": "^4.18.1",
    "caniuse-lite": "^1.0.30001282",
    "dodex": "^1.4.0",
    "dodex-input": "^1.4.0",
    "dodex-mess": "^1.3.1",
    "handlebars": "^4.7.7",
    "helpers": "0.0.6",
    "jquery": "^3.6.0",
    "jsoneditor": "^9.5.7",
    "lodash-es": "^4.17.21",
    "marked": "^4.0.10",
    "moment": "^2.29.1",
    "tablesorter": "^2.31.3",
    "vue": "^3.2.22",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@metahub/karma-jasmine-jquery": "^4.0.1",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/cli-service": "^4.5.15",
    "@vue/compiler-sfc": "^3.2.22",
    "@vue/component-compiler-utils": "^3.3.0",
    "@vue/test-utils": "^2.0.0-rc.16",
    "concurrently": "^6.4.0",
    "copyfiles": "^2.4.1",
    "cypress": "^9.0.0",
    "cypress-webpack-preprocessor-v5": "^5.0.0-alpha.1",
    "karma": "^6.3.9",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "^4.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-opera-launcher": "^1.0.0",
    "npm-watch": "^0.11.0",
    "rxjs": "^6.6.7",
    "start-server-and-test": "^1.14.0",
    "vite": "^2.6.14",
    "vue-loader": "^16.8.3",
    "webpack": "^5.64.2",
    "webpack-dev-server": "^4.5.0"
  },
  "browser": {
    "app": "./src/appl/js/app.js",
    "basecontrol": "./src/appl/js/utils/base.control.js",
    "config": "./src/appl/js/config.js",
    "default": "./src/appl/js/utils/default.js",
    "helpers": "./src/appl/js/utils/helpers.js",
    "menu": "./src/appl/js/utils/menu.js",
    "pager": "./node_modules/tablesorter/dist/js/extras/jquery.tablesorter.pager.min.js",
    "pdf": "./src/appl/js/controller/pdf.js",
    "router": "./src/appl/js/router",
    "setup": "./src/appl/js/utils/setup.js",
    "start": "./src/appl/js/controller/start.js",
    "table": "./src/appl/js/controller/table.js",
    "apptest": "./src/appl/jasmine/apptest.js",
    "contacttest": "./src/appl/jasmine/contacttest.js",
    "domtest": "./src/appl/jasmine/domtest.js",
    "logintest": "./src/appl/jasmine/logintest.js",
    "routertest": "./src/appl/jasmine/routertest.js",
    "toolstest": "./src/appl/jasmine/toolstest.js",
    "dodextest": "./src/appl/jasmine/dodextest.js",
    "inputtest": "./src/appl/jasmine/inputtest.js",
    "setglobals": "./src/appl/js/utils/set.globals.js",
    "setimports": "../src/appl/js/utils/set.imports.js",
    "startc": "./src/appl/components/StartC.vue",
    "starta": "./src/appl/components/StartA.vue",
    "helloworld": "./src/appl/components/HelloWorld.vue",
    "toolsc": "./src/appl/components/ToolsC.vue",
    "pdfc": "./src/appl/components/PdfC.vue",
    "dodexc": "./src/appl/components/DodexC.vue",
    "contactc": "./src/appl/components/ContactC.vue",
    "notfound": "./src/appl/components/NotFound.vue",
    "toolsAsync": "./src/appl/components/ToolsAsyncC.vue",
    "@": "./src/appl"
  },
  "watch": {
    "karma": {
      "patterns": [
        "touch.file"
      ],
      "extensions": "js,vue,md,html",
      "ignore": "node_modules",
      "quiet": true,
      "legacyWatch": true,
      "delay": 1000,
      "runOnChangeOnly": true
    }
  }
}
