{
  "name": "embedded-acceptance-tests-vue",
  "version": "1.0.0",
  "description": "Vue acceptance testing",
  "keywords": [
    "karma",
    "jasmine",
    "vuejs",
    "bootstrap",
    "browserify",
    "brunch",
    "fusebox",
    "stealjs",
    "steal-tools"
  ],
  "author": "DaveO",
  "private": true,
  "scripts": {
    "server": "node ../appExpress.js",
    "lint": "eslint --ext .js,.vue --ignore-pattern marked.js --ignore-pattern webcomponents-hi-sd-ce.js",
    "bpc": "rm -rf dist/brunch",
    "btc": "rm -rf dist_test/brunch",
    "bt": "npm run btc && brunch b --env test",
    "bp": "export LOGGY_STACKS=1; npm run bpc && brunch build --production",
    "bw": "npm run btc && brunch watch --server"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.2.0-6",
