    "tablesorter": "^2.31.3",
    "vue": "^2.6.14",
    "vue-router": "^3.5.3",
    "vue-to-js": "^0.9.4",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
